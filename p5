/*Write a Java program to demonstrate multilevel Inheritance*/

220130318010
Vediya Khushi R.

import java.util.*;

class Shape
 {
   public void display()
 {
      System.out.println("It's display");
   }
}
class Rectangle extends Shape {
   public void area() 
{
      System.out.println("It's area");
   }
}
class Cube extends Rectangle
 {
   public void volume()
 {
      System.out.println("It's volume");
   }
}
public class P3_5
{
   public static void main(String[] args)
 {
      Cube c1 = new Cube();
      c1.display();
      c1.area();
      c1.volume();
   }
}
