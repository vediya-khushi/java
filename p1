/*Write a Java Program to explain super keyword. */

220130318010
VEdiya Khushi R.

import java.util.*;

class Vehicle                                                                           
{ 
	int maxSpeed = 120; 
} 

class Car extends Vehicle
 { 
	int maxSpeed = 180; 

	void display() 
	{ 
		System.out.println("Maximum Speed: "+ super.maxSpeed); 
	} 
} 

public class P3_1
 { 
	public static void main(String[] args) 
	{ 
		Car c1 = new Car(); 
		c1.display(); 
	} 
}

